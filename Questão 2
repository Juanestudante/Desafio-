include <stdio.h>
#include <string.h>

// Definição da estrutura para representar as informações financeiras
typedef struct {
    char data[11];   // Formato: DD/MM/AAAA
    float valor;
    char tipo[20];
    char nome[20];
} InformacaoFinanceira;

int main() {
    // Criando e inicializando uma instância da estrutura
    InformacaoFinanceira info1;
    
    strcpy(info1.data, "22/02/2020");
    info1.valor = 913.00;
    strcpy(info1.tipo, "Renda Fixa");
    strcpy(info1.nome, "CDB maj");

